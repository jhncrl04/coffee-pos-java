              <AuxValues>
                <AuxValue name="JavaCodeGenerator_AddingCodePost" type="java.lang.String" value="receiptTA.setDisabledTextColor(new java.awt.Color(113, 96, 64));&#xa;&#xa;receiptTA.setText(&quot;   Product&quot; + &quot;\t\t&quot; + &quot;Size&quot; + &quot;\t&quot; + &quot;Quantity&quot; + &quot;\t&quot; + &quot;Price&quot; + &quot;\n&quot;);&#xa;receiptTA.setText(receiptTA.getText() + &quot;============================================================\n\n&quot;);&#xa;&#xa;for(int i = 0; i &lt; bagContent.getRowCount(); i++){&#xa;    &#xa;    String productName = bagContent.getValueAt(i, 0).toString();&#xa;    String productSize = (productName.length() &lt; 11) ? &quot;\t&quot; + bagContent.getValueAt(i, 1).toString(): bagContent.getValueAt(i, 1).toString();&#xa;    String quantity = bagContent.getValueAt(i, 2).toString();&#xa;    String price = &quot;PHP &quot; + String.valueOf(Menu.decimalFormat.format( Double.parseDouble(bagContent.getValueAt(i, 3).toString())));&#xa;    &#xa;    receiptTA.setText(receiptTA.getText() + &quot;   &quot; + productName + &quot;\t&quot; + productSize + &quot;\t&quot; + quantity + &quot;\t&quot; + price + &quot;\n&quot;);&#xa;}&#xa;    receiptTA.setText(receiptTA.getText() + &quot;\n============================================================\n&quot;);&#xa;    receiptTA.setText(String.format(receiptTA.getText() + &quot;   \t\t\t           Total: \tPHP %s\n&quot;, Menu.totalPriceInBag));"/>
              </AuxValues>
            </Component>
